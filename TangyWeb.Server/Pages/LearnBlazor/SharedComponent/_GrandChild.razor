<div class="row">
    <h4 class="text-primary pl-4 pt-2 col-12">Grand Child</h4>
    <p>There is message - @MessageForGrandChild</p>
    <p>Lucky Number - @LuckyNumberForGrandChild</p>
</div>

@code {
    [CascadingParameter(Name = "message")]
    public string MessageForGrandChild { get; set; }
    [CascadingParameter(Name = "lucky")]
    public string LuckyNumberForGrandChild { get; set; }
    /*if multiple cascading parameter and type is different
    [CascadingParameter]
    public int LuckyNumberForGrandChild { get; set; }*/
}
